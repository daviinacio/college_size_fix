{
    "name": "Estácio Flash Size",
    "version": "1.0",
    "manifest_version": 2,
    "author": "daviinacio",
    "description": "Uma forma simples e rápida de ajustar o tamanho do conteúdo em flash da Estácio, e facilitar a leitura do mesmo.",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Selecione o tamanho"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
       "128": "icon.png"
    }
}